<form method="post" action="/users/login">
  <div>
    <label>Username:</label>
    <input name="username" type="text" value="<%= params[:username] %>"><br>
  </div>
  <div>
    <label>Password:</label>
    <input name="password" type="text"><br>
  </div>
  <button type="submit">Sign In</button>
</form>